{
    "name": "xolens/larasetting",
    "description": "Laravel setting management package.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Xolens",
        "email": "xolens.official@gmail.com"
    }],
    "require": {
        "xolens/larautil": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Xolens\\Larasetting\\": "src/Larasetting",
            "Xolens\\Larasetting\\Model\\": "src/Larasetting/Model/",
            "Xolens\\Larasetting\\Util\\": "src/Larasetting/Util/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larasetting\\LarasettingServiceProvider"
            ]
        }
    }
}